<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletService</servlet-name>
  	<display-name>ServletService</display-name>
  	<description></description>
  	<servlet-class>com.myCompany.ServletService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListControllerServlet</servlet-name>
  	<display-name>ListControllerServlet</display-name>
  	<description></description>
  	<servlet-class>com.myCompany.ListControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletService</servlet-name>
  	<url-pattern>/ServletService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListControllerServlet</servlet-name>
  	<url-pattern>/ListControllerServlet</url-pattern>
  </servlet-mapping>
</web-app>
